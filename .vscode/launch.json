    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Attach",
            "type": "cppvsdbg",
            "request": "attach",
            "processId": "${command:pickProcess}",
        },
        {
            "name": "Debug Launch",
            "type": "cppvsdbg",
            "request": "launch",
            "cwd": "${workspaceRoot}/.install/app/bin",
            "program": "${workspaceRoot}/.install/app/bin/app.exe",
        }
    ]
}